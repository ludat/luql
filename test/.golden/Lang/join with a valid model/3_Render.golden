SELECT "t".*, "city".* FROM (SELECT "country"."country" AS "country.country", "country"."country_id" AS "country.country_id", "country"."last_update" AS "country.last_update" FROM "country") AS "t" JOIN (SELECT "city"."city" AS "city.city", "city"."city_id" AS "city.city_id", "city"."country_id" AS "city.country_id", "city"."last_update" AS "city.last_update" FROM "city") AS "city" ON ("country.country_id" = "city.country_id")