Right
  RawQuery
    { unRawQuery =
        [ From
            Range { begin = 0 , end = 15 }
            ( Just "l" , Ref Range { begin = 5 , end = 15 } "Languages" )
        , Return
            Range { begin = 20 , end = 40 }
            [ Prop
                Range { begin = 28 , end = 40 }
                (Ref Range { begin = 27 , end = 28 } "l")
                "language_id"
            ]
        ]
    }