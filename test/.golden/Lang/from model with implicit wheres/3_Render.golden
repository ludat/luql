SELECT "t".* FROM (SELECT "customer"."active" AS "customer.active", "customer"."activebool" AS "customer.activebool", "customer"."address_id" AS "customer.address_id", "customer"."create_date" AS "customer.create_date", "customer"."customer_id" AS "customer.customer_id", "customer"."email" AS "customer.email", "customer"."first_name" AS "customer.first_name", "customer"."last_name" AS "customer.last_name", "customer"."last_update" AS "customer.last_update", "customer"."store_id" AS "customer.store_id" FROM "customer") AS "t" WHERE ("customer.active" = 1)