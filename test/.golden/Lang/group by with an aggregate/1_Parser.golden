Right
  RawQuery
    { unRawQuery =
        [ From
            Range { begin = 0 , end = 11 }
            ( Nothing , Ref Range { begin = 5 , end = 11 } "Cities" )
        , GroupBy
            Range { begin = 12 , end = 18 }
            [ ( Prop
                  Range { begin = 25 , end = 37 }
                  (Ref Range { begin = 21 , end = 25 } "city")
                  "country_id"
              , Nothing
              )
            ]
            [ Let
                Range { begin = 41 , end = 76 }
                "max_city"
                (Apply
                   Range { begin = 59 , end = 76 }
                   (Ref Range { begin = 56 , end = 59 } "max")
                   [ Prop
                       Range { begin = 64 , end = 75 }
                       (Ref Range { begin = 60 , end = 64 } "city")
                       "country_id"
                   ])
            ]
        ]
    }