SELECT "t".*, "film".* FROM (SELECT "language"."language_id" AS "l.language_id", "language"."last_update" AS "l.last_update", "language"."name" AS "l.name" FROM "language") AS "t" JOIN (SELECT "film"."description" AS "film.description", "film"."film_id" AS "film.film_id", "film"."language_id" AS "film.language_id", "film"."title" AS "film.title" FROM "film") AS "film" ON ("l.language_id" = "film.language_id")